{
  "name": "my-cv",
  "version": "0.1.0",
  "private": true,
  "description": "This repository contains all of my cv web",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "SET ENVIRONMENT=production&& webpack -p --progress",
    "start": "SET ENVIRONMENT=development&& webpack-dev-server -d --progress",
    "deploy-aws": "aws s3 sync build/ s3://my-cv",
    "mac": "ENVIRONMENT=development webpack-dev-server -d --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "author": "Wozzo",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaviGF8/my-cv.git"
  },
  "bugs": {
    "url": "https://github.com/JaviGF8/my-cv/issues"
  },
  "homepage": "https://javigf8.github.io/my-cv/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^5.0.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
